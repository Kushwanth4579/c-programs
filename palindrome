//palindrome
#include<stdio.h>
main()

{
	int a,b,c,s,x;
	printf("enter a number ");
	scanf("%d",&a);
	x=a;
    
    for(c=0,s=0;a>=1; )
    {
    	b=a%10;
    	s=s*10+b;
    	a/=10;
	}
	printf("reverse is %d so\n",s);
	if (s==x)
	 printf("it's a palindrome");
	else
	 printf("it's not a palindrome");
}
