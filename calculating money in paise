//calculating amount in paise
#include <stdio.h>
int main()
{
	float a,b,c,d,e,x;
	printf("enter 50,25,20,10,5");
	scanf("%f%f%f%f%f",&a,&b,&c,&d,&e);
	x=(a*50+b*25+c*20+d*10+e*5)/100;
	printf("total amount in piggy bank is %.2f rupees",x);
	printf("total amount in piggy bank is %0.2f paise",x*100);
	return 0;
}
